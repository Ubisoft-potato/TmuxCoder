    "lint": "eslint src"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/handlebars": "^4.1.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0"
  },
  "keywords": ["tmuxcoder", "prompt", "llm", "ai"],
  "license": "MIT"
}
