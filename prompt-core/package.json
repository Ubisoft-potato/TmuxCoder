{
  "name": "@tmuxcoder/prompt-core",
  "version": "0.1.0",
  "description": "TmuxCoder Prompt Management SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
