{
  "mode": "local",
  "promptProxy": {
    "enabled": false,
    "overrideSystem": true,
    "overrideParams": true
  },
  "monkeyPatch": {
    "enabled": false,
    "interceptEnvironment": true,
    "interceptCustom": true
  },
  "logging": {
    "level": "info",
    "enableMetrics": true,
    "enableTracing": false
  },
  "cache": {
    "enabled": true,
    "ttl": 300,
    "maxSize": 100
  },
  "debug": false
}
